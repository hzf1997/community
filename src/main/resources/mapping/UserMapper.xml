<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hzf.community.dao.UserDao">
    <select id="findByToken" parameterType="String" resultType="User">
        select * from user where token=#{token}
    </select>
    <insert id="insert" parameterType="User">
        INSERT into user (accountId,name,token,gmtCreate,gmtModified) VALUES (#{accountId},#{name},#{token},#{gmtCreate},#{gmtModified})
    </insert>
</mapper>